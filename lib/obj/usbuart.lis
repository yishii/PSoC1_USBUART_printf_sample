 0301           ;    of this function.  The same is true for all RAM page pointer registers in
 0301           ;    the Large Memory Model.  When necessary, it is the calling function's
 0301           ;    responsibility to preserve their values across calls to fastcall16 
 0301           ;    functions.
 0301           ;          
 0301           ;    Currently only the page pointer registers listed below are modified: 
 0301           ;          CUR_PP
 0301           ;          IDX_PP
 0301           ;
 0301           ; -2 and -3 is the return address to caller
 0301            USBUART_SendStateNotify:
 0301           _USBUART_SendStateNotify:
                   IF ( RAM_USE_CLASS_1 & RAM_USE_CLASS_1 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_1
                
                   IF ( RAM_USE_CLASS_1 & RAM_USE_CLASS_2 )
                      IF ( SYSTEM_IDXPG_TRACKS_IDX_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      or   F, FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_2
                
                   IF ( RAM_USE_CLASS_1 & RAM_USE_CLASS_3 )
                      IF ( SYSTEM_IDXPG_TRACKS_STK_PP )
                   IF ( SYSTEM_LARGE_MEMORY_MODEL )
                      and  F, ~FLAG_PGMODE_01b
                   ENDIF ;  SYSTEM_LARGE_MEMORY_MODEL
                      ENDIF
                   ENDIF ; RAM_USE_CLASS_3
                
                   IF ( RAM_USE_CLASS_1 & RAM_USE_CLASS_4 )
                   ; Nothing to do
                   ENDIF ; RAM_USE_CLASS_4
                
                    ;Should we toggle the data bit??
                    ;SET THE COUNT AND TOGGLE
 0301 08                push    A
 0302 55000A            mov     [USBUART_APITemp], 10         ; Save the count
 0305 360180            xor     [USBUART_EPDataToggle+1], USB_CNT_TOGGLE
 0308           
 0308 624F0A        mov     reg[USBUART_EP1CNT0], 10    ; Write it into the register
 030B 5101              mov     A, [USBUART_EPDataToggle+1]    ; Retrieve the saved toggle (MSB)
 030D 604E              mov     reg[USBUART_EP1CNT1], A        ; Write it into the register
 030F               
 030F           ; It's Time to move the data
 030F           ; First we need to determine where, within the PMA, the EP Start Address is
 030F 5D6C              mov     A, reg[TMP_DR0]                ; Get the address of ep from tmp register 

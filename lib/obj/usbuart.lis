 031A 624020            mov             reg[PMA0_DR], 20h       ; Serial State
 031D 624000            mov             reg[PMA0_DR], 0         ; wValue MSB
 0320 624000            mov             reg[PMA0_DR], 0         ; wValue LSB
 0323 624000            mov             reg[PMA0_DR], 0         ; wIndex MSB
 0326 624000            mov             reg[PMA0_DR], 0         ; wIndex (Interface)
 0329 624000            mov             reg[PMA0_DR], 0         ;wLen
 032C 624002            mov             reg[PMA0_DR], 2         ;wLen
 032F 624000            mov             reg[PMA0_DR], 0         ;wLen
 0332 18                pop             A
 0333 6040              mov             reg[PMA0_DR], A         ;State bitmap
 0335                   
 0335 550100        mov     [USBUART_EndpointAPIStatus+1], NO_EVENT_PENDING ; Set the state
 0338 7110          or    F, FLAG_XIO_MASK
 033A 5D6C              mov     A, reg[TMP_DR0]               ; Get the value of the PMA start Address                                                          
 033C 6051              mov     reg[PMA0_RA+1], A             ; Load it into EP PMA so pre-fetch occurs                                          
 033E 62C40D        mov     reg[USBUART_EP1MODE], USB_MODE_ACK_IN ; Enable the endpoint
 0341 70EF          and   F, ~FLAG_XIO_MASK
                   IF ( RAM_USE_CLASS_1 & RAM_USE_CLASS_1 )
                   ; Nothing to do
